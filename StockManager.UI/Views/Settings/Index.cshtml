            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Season)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WWW)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VatRate)
            </td>
            <td>
